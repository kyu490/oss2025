#정수 명령 처리
#include <stdio.h>
#include <stdlib.h>
#include <string.h>

#define MAX 10000
int stack[MAX];
int top = -1;

void push(int x){
  stack[++top] = x;
}

void pop(){
  if (top == -1)
    printf("-1\n");
  else 
    printf("%d\n", stack[top--]);
}

void size(){
  printf("%d\n", top + 1);
}

void empty(){
  printf("%d\n", top == -1 ? 1 : 0);
}

void top_element(){
  if (top == -1)
    printf("-1\n");
  else
    printf("%d\n", stack[top]);
}

int main(){
  int n;
  char command[20];

  scanf("%d", &n);
  for (int i = 0; i < n; i++){
    scanf("%s", command);

    if(strcmp(command, "push") == 0) {
      int x; 
      scanf("%d", &x);
      push(x);
    }
    else if (strcmp (command, "pop") == 0){
      pop();
    }
    else if (strcmp (command, "size") == 0){
      size();
    }
    else if (strcmp (command, "empty") == 0){
      empty();
    }
    else if (strcmp (command, "top") == 0){
      top_element();
    }
  }

  return 0;
}

#정수 명령 처리2
